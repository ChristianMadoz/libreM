{
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "index.py"
        },
        {
            "source": "/((?!api|static|favicon.ico|logo.*|manifest.json|asset-manifest.json|robots.txt).*)",
            "destination": "/index.html"
        }
    ]
}